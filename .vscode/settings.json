{
	"workbench.colorTheme": "Winter is Coming (Dark Blue)",
	"cSpell.language": "en,es",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[python]": {
		"editor.defaultFormatter": "ms-python.autopep8"
	},
	"editor.formatOnType": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.wordWrap": "on",
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"html.format.wrapLineLength": 150,
	"workbench.settings.editor": "json",
	"git.enableSmartCommit": true,
	"editor.tabCompletion": "on",
	"editor.lineHeight": 20,
	"editor.cursorBlinking": "expand",
	"workbench.iconTheme": "material-icon-theme",
	"material-icon-theme.folders.color": "#7cb342",
	"files.associations": {
		"*.feature": "feature"
	},
	"material-icon-theme.files.associations": {
		"test.py": "Test-jsx",
		"environment.py": "Test-ts",
		"*.json": "3d",
		".feature": "cucumber",
		"cucumber-report.json": "cucumber",
		"settings.json": "Vscode",
		"requirements.txt": "todo",
		"*.yml": "commitlint"
	},
	"material-icon-theme.folders.associations": {
		"api-testing": "api",
		"db-testing": "database",
		"ui-testing": "app",
		"drivers": "benchmark",
		"steps": "test",
		"features": "Resolver",
		"cucumber-json": "Resolver",
		"DOM": "class",
		"How_to": "content",
		"US": "Storybook",
		"Tests": "Test",
		"Test_Plan": "docs",
		"smoke": "core",
		"sanity": "target",
		".conda": "packages",
		"regression": "ci",
		"in-sprint": "Storybook",
		"fixtures": "container",
		"support": "Helper",
		"xhr-fetch": "batch",
		"material-icons": "examples",
		"workflows": "github"
	},
	"cypressHelper.menuItems": {
		"OpenCypress": false,
		"RunCypress": false,
		"GenerateCustomCommandTypes": true,
		"GoToCustomCommand": false,
		"FindCustomCommandReferences": true,
		"FindStepDefinitionReferences": true,
		"SchemaGenerator": false,
		"ParseHAR": false,
		"mockAutocomplete": false
	},
	"python.terminal.activateEnvironment": true,
	"python.testing.unittestEnabled": false,
	"python.testing.pytestEnabled": true,
	"python.testing.pytestArgs": ["."],
	"python.analysis.typeCheckingMode": "basic"
}
